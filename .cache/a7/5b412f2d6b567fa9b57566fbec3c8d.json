\r\n","css\\_landing.scss":".landing {\r\n  min-height: 100vh;\r\n  max-width: 100vw;\r\n  background: url(\"./../img/cool\\ beachjpg.webp\") no-repeat;\r\n  background-position: 0 -150px;\r\n  background-size: auto auto;\r\n  background-color: rgba(0, 0, 0, 0.3);\r\n  background-blend-mode: hard-light;\r\n  overflow: hidden;\r\n  display: flex;\r\n  flex-direction: column;\r\n  position: relative;\r\n  z-index: 1;\r\n\r\n  .blurE {\r\n    background: transparent;\r\n    transition: all 0.3s ease-in-out;\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 0;\r\n  }\r\n\r\n  .z {\r\n    z-index: 9 !important;\r\n  }\r\n\r\n  .heading {\r\n    position: relative;\r\n    border-radius: 10px;\r\n    width: auto;\r\n    margin: 2.5rem auto 2rem;\r\n    height: auto;\r\n    z-index: 10;\r\n    transition: all 0.4s ease-in-out;\r\n\r\n    &:hover {\r\n      li a i {\r\n        color: $secondary;\r\n      }\r\n    }\r\n\r\n    li {\r\n      transition: inherit;\r\n      position: relative;\r\n      padding: 1rem;\r\n\r\n      a {\r\n        position: relative;\r\n        padding: 5px;\r\n        color: $primary;\r\n        font-size: 1.5rem;\r\n        transition: all 0.2s ease-in-out;\r\n\r\n        i {\r\n          transition: inherit;\r\n        }\r\n\r\n        &:hover i {\r\n          color: $third;\r\n        }\r\n\r\n        i:hover {\r\n          transform: scale(2);\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .showcase {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    // flex-grow: 1;\r\n    text-align: center;\r\n    height: auto;\r\n    width: auto;\r\n    flex-direction: column;\r\n\r\n    .text {\r\n      // background-image: linear-gradient(\r\n      //   to right,\r\n      //   transparent 0%,\r\n      //   rgba(0, 0, 0, 0.5) 10%,\r\n      //   rgba(0, 0, 0, 0.5) 90%,\r\n      //   transparent 100%\r\n      // );\r\n      // background: rgba(0, 0, 0, 0.5);\r\n      border-radius: 10px;\r\n      // padding: 1.5rem;\r\n      flex-direction: column;\r\n\r\n      .title {\r\n        color: $third;\r\n        font-size: 4rem;\r\n        letter-spacing: 1.1rem;\r\n        transform: skewX(-10deg);\r\n        cursor: context-menu;\r\n        margin-bottom: 1px;\r\n      }\r\n      .description {\r\n        display: block;\r\n        margin-top: 2rem;\r\n        font-size: 1.5rem;\r\n        color: $third;\r\n        transition: all 0.4s ease-in-out;\r\n        font-weight: 500;\r\n        padding: 1rem;\r\n        border-radius: 50px;\r\n        background: rgba(202, 240, 248, 0.7);\r\n        animation: up-down;\r\n        animation-duration: 3s;\r\n        animation-iteration-count: infinite;\r\n        border: 1px solid rgba(202, 240, 248, 0.9);\r\n\r\n        &:hover {\r\n          background: rgba(202, 240, 248, 0.9);\r\n          color: rgb(22, 108, 165);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n","css\\_near.scss":".near-by {\r\n  width: 100%;\r\n  height: auto;\r\n\r\n  .container {\r\n    width: 90%;\r\n    margin: 1rem auto;\r\n    display: grid;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c512691d22cdceada1b87f9357abdc04","cacheData":{"env":{}}}